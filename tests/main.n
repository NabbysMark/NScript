YE mylist BOOM [1, 2, 3, 4]
YE mydict BOOM {"a": 10, "b": 20, "c": 30}
YE mystr BOOM "abcdefg"
YE mylist BOOM [10, 20, 30, 40, 50, 60, 70]

// Loop through a list
SPIN BOOM idx, val IN mylist WE
    FELLA `List index: ${idx}, value: ${val}`
POW

// Loop through a dictionary
SPIN BOOM key, val IN mydict WE
    FELLA `Dict key: ${key}, value: ${val}`
POW

// Indexing single element (1-based)
FELLA mystr[2]         // prints "b"
FELLA mylist[3]        // prints 30

// Slicing (like Python, but 1-based for start/stop)
FELLA mystr[2:5]       // prints "bcd"
FELLA mylist[2:5]      // prints [20, 30, 40]

// Slicing with step
FELLA mystr[1:7:2]     // prints "aceg"
FELLA mylist[1:7:3]    // prints [10, 40, 70]

// Open-ended slices
FELLA mystr[:4]        // prints "abcd"
FELLA mylist[4:]       // prints [40, 50, 60, 70]
FELLA mystr[:]         // prints "abcdefg"